            <Insets bottom="20" left="20" right="20" top="20" />
         </padding>
         <TextField fx:id="nameField" alignment="CENTER" maxWidth="300" promptText="Account Name" />
         <TextField fx:id="balanceField" alignment="CENTER" maxWidth="300" promptText="Opening Balance" />
         <DatePicker fx:id="datePicker" maxWidth="300" />
