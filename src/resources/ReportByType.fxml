<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ReportByTypeController">
    <children>
        <ComboBox fx:id="transactionTypeDropdown" layoutX="20.0" layoutY="20.0" prefWidth="300.0" promptText="Select Transaction Type" />
        <TableView fx:id="transactionTable" layoutX="20.0" layoutY="70.0" prefWidth="800.0" prefHeight="400.0">
            <columns>
                <TableColumn fx:id="accountColumn" text="Account" prefWidth="150.0" />
                <TableColumn fx:id="transactionDateColumn" text="Transaction Date" prefWidth="200.0" />
                <TableColumn fx:id="transactionDescriptionColumn" text="Description" prefWidth="250.0" />
                <TableColumn fx:id="paymentAmountColumn" text="Payment Amount" prefWidth="100.0" />
                <TableColumn fx:id="depositAmountColumn" text="Deposit Amount" prefWidth="100.0" />
            </columns>
        </TableView>
        <Button fx:id="backButton" layoutX="20.0" layoutY="490.0" text="Back" onAction="#goBack" />
    </children>
</AnchorPane>
